object HelloWorld {
  def main(args: Array[String]): Unit = {
    var sentence : String = "Hello Scala!"
    println(sentence)
    
    var i = 0
    for (i <- 0 to 9) {
      println(sentence.replaceFirst(sentence.charAt(i).toString, "X"))
    }
  }
}